{
    "manifest_version": 3,
    "name": "Window Title Copier",
    "version": "1.0",
    "permissions": ["activeTab", "clipboardWrite"],
    "action": {
      "default_popup": "popup.html"
    },
    "icons": {
      "16": "icon-16.png",
      "48": "icon-48.png",
      "128": "icon-128.png"
    }
  }
  